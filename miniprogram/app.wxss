/**app.wxss**/
page {
  --white: #fff;
  --grey: #84868c;
  --brown:  #9c774e;
  --black-dark: #1e1e24;
  --black-light: #292b33;
  
  --border: 2rpx solid var(--black-light);

  --nav-index: 99;

  color: var(--white);
  background: var(--black-dark);
}

@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1620618_395u6f86sle.eot?t=1579596656162'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1620618_395u6f86sle.eot?t=1579596656162#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_1620618_395u6f86sle.woff?t=1579596656162') format('woff'),
  url('//at.alicdn.com/t/font_1620618_395u6f86sle.ttf?t=1579596656162') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_1620618_395u6f86sle.svg?t=1579596656162#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-back:before {
  content: "\e61c";
}
